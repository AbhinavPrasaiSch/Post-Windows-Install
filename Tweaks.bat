@Echo Off
Title RekOS Optimization Tweaks

echo Applying bcdedit tweaks

timeout 1 >nul 

bcdedit /set disabledynamictick yes

bcdedit /deletevalue useplatformclock

bcdedit /set useplatformtick yes

Reg.exe add "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Multimedia\SystemProfile" /v "SystemResponsiveness" /t REG_DWORD /d "0" /f

cls

echo Disabling Power Saving Features on USB and Ethernet

timeout 1 >nul

powershell.exe -encodedCommand 

cls

echo Running netsh cmds

timeout 1 >nul

ipconfig /release

ipconfig /renew

ipconfig /flushdns

netsh winsock reset

cls

echo Separating Audio Service (Credit Unixcorn)

timeout 1 >nul

copy C:\Windows\System32\svchost.exe C:\Windows\System32\audiosvchost.exe
reg add "HKLM\SYSTEM\CurrentControlSet\Services\Audiosrv" /v ImagePath /t REG_EXPAND_SZ /d "%SystemRoot%\System32\audiosvchost.exe -k LocalServiceNetworkRestricted" /f

